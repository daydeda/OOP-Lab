@startuml
' Account base class
class Account {
  - name: String
  - id: int
  - balance: double
  - annualInterestRate: double
  - dateCreated: Date
  - {static} idCounter: int = 1000
  + Account(name: String)
  + deposit(amount: double): void
  + withdraw(amount: double): void
  + getBalance(): double
  + setBalance(balance: double): void
  + toString(): String
}

' CheckingAccount subclass
class CheckingAccount {
  - overdraftLimit: double = 5000
  + CheckingAccount(name: String)
  + withdraw(amount: double): void
  + toString(): String
}

' SavingAccount subclass
class SavingAccount {
  - count: int = 0
  - {final} LIMIT: int = 5
  + SavingAccount(name: String)
  + withdraw(amount: double): void
  + toString(): String
}

CheckingAccount --|> Account
SavingAccount --|> Account
@enduml
